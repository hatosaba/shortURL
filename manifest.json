{
    "manifest_version": 3,
    "name": "hat.f5.si Short URL",
    "description": "",
    "version": "0.0.2",
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "run_at":"document_start",
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "permissions": [
        "tabs",
        "activeTab",
        "clipboardWrite",
        "contextMenus"
    ],
    "icons": {
        "16": "assets/favicon.ico",
        "48": "assets/favicon.ico",
        "128": "assets/favicon.ico"
    }
}
